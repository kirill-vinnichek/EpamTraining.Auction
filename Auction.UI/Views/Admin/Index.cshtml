@model IEnumerable<Auction.Model.Models.UserViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="container">
    <!-- Title Page -->
    <div class="row">
        <div class="span12">
            <div class="title-page">
                <h2 class="title">Welcome Admin</h2>
            </div>
        </div>
    </div>
    <!-- End Title Page -->
    <!-- Portfolio Projects -->
    <div class="row">
        <div class="span3">
            <!-- Filter -->
            <nav id="options" class="work-nav">
                <ul id="filters" class="option-set">
                    <li class="type-work">Users</li>
                    @if (Model != null)
                    {
                        foreach (var u in Model)
                        {
                            <li><a href="@Url.Action("EditUser","Admin",new { id = u.UserId})">@u.DisplayName - @u.Email</a></li>
                        }
                    }
                       
                </ul>
            </nav>
        </div>

    </div>
</div>





